;
[cwcspud]
CopyFiles=cwcspud.CopyList, CS4610DSP.CopyList
AddReg=CSCAud.cwcspud.AddReg, cwcspud.AddReg
DriverVer=07/01/2001,5.12.01.3056
;KnownRegEntries=cwcspud.KnownRegEntries

;[cwcspud.KnownRegEntries]
