;[IsWin98Gold]
;1=HKLM,Software\Microsoft\Windows\CurentVersion,VersionNumber,0,4.10.1998

[cwcspud.Interfaces]
AddInterface=%CRYSTAL_SPUD_GUID%,%CRYSTAL_SPUD%,CSCAud.Interface.SPuD
