[cwcwdm.Interfaces]
AddInterface=%MSKSCATEGORY_AUDIO%,%KSNAME_Wave%,CSCAud.Interface.Wave
AddInterface=%MSKSCATEGORY_RENDER%,%KSNAME_Wave%,CSCAud.Interface.Wave
