HKLM,%KEY_BASE%\Inst,WinDir,,%10%
HKLM,%KEY_BASE%\Inst,File0,,%10%\cwcdata\cwcspud.dat
HKLM,%KEY_BASE%\Trans,WinDir,,%10%
